{
  "name": "mona-dish",
  "version": "0.20.0",
  "description": "TypeScript/Javascript set of functional pattern inspired helpers, which will ease your programming live",
  "main": "dist/js/commonjs/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register -P ./src/ ./src/test/typescript/**/*Test.ts",
    "coverage": "nyc --reporter=html --reporter=text mocha -r ts-node/register ./**/test/typescript/**/**/*Test*.ts",
    "build-umd": "webpack --env TARGET_TYPE=umd",
    "build-window": "webpack --env TARGET_TYPE=window",
    "build-common": "webpack --env TARGET_TYPE=commonjs",
    "build-system": "webpack --env TARGET_TYPE=system",
    "build-amd": "webpack --env TARGET_TYPE=amd",
    "build-amd-require": "webpack --env TARGET_TYPE=amd-require",
    "build": "npm run build-umd && npm run build-common && npm run build-system && npm run build-amd && npm run build-amd-require"
  },
  "keywords": [
    "typescript",
    "javascript",
    "monad"
  ],
  "author": "Werner Punz",
  "license": "Apache-2.0",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^10.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "broadcast-channel": "^3.5.3",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^8.1.1",
    "filemanager-webpack-plugin-fixed": "^2.0.9",
    "html-webpack-plugin": "^5.3.1",
    "jsdom": "^16.5.3",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "request": "^2.88.2",
    "rxjs": "^6.6.7",
    "single-spa": "^5.9.2",
    "sinon": "^10.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "npm-check-updates": "^11.4.1"
  }
}

{
  "name": "mona-dish",
  "version": "0.13.0",
  "description": "TypeScript/Javascript set of functional pattern inspired helpers, which will ease your programming live",
  "main": "dist/js/commonjs/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register -P ./src/ ./src/test/typescript/**/*Test.ts",
    "coverage": "nyc --reporter=html --reporter=text mocha -r ts-node/register ./**/test/typescript/**/**/*Test*.ts",
    "build": "TARGET_TYPE=commonjs webpack",
    "build-umd": "TARGET_TYPE=umd webpack",
    "build-window": "TARGET_TYPE=window webpack",
    "build-common": "TARGET_TYPE=commonjs webpack",
    "build-system": "TARGET_TYPE=system webpack",
    "build-amd": "TARGET_TYPE=amd webpack",
    "build-amd-require": "TARGET_TYPE=amd-require webpack",
    "build-all": "npm run build-umd && npm run build-common && npm run build-system && npm run build-amd && npm run build-amd-require"
  },
  "keywords": [
    "typescript",
    "javascript",
    "monad"
  ],
  "author": "Werner Punz",
  "license": "Apache-2.0",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.5.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.5",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.2.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "sinon": "^7.5.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "npm-check-updates": "^3.2.0"
  }
}
